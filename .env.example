# Ejemplo de .env para habilitar Claude Haiku 4.5

# Habilita la funcionalidad para todos los clientes (true/false)
ENABLE_CLAUDE_HAIKU=true

# Modelo a usar por defecto
ANTHROPIC_MODEL=claude-haiku-4.5

# Tu clave de Anthropic (no subir al repositorio)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
